<h4>会員一覧</h4>

<table>
  <thead>
    <tr>
      <th>会員ID</th>
      <th>ニックネーム</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.id %></td>
        <td>
          <%= link_to admin_member_path(member.id) do %>
            <%= member.nick_name %>
          <% end %>
        </td>
        <td><%= member.email %></td>
        <td>
          <% if member.is_deleted == false %>
            <strong>有効</strong>
          <% elsif %>
            <strong>退会済</strong>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table